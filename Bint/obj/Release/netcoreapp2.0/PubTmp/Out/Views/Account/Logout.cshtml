
@{
   
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>

    /*ipad*/ 
    @@media (min-width: 768px) {

        .row {
            margin-top: 55%;
        }
    }

    @@media (min-width: 768px) and (orientation:landscape) {

        .row {
            margin-top: 30%;
        }
    }


    /*mobile*/ 
    @@media (max-width: 767px) {

        .row {
            margin-top: 55%;
        }
    }

    @@media (max-width: 767px)and (orientation:landscape) {

        .row {
            margin-top: 18%;
        }
    }

    /*laptop and other screens */
    @@media (min-width: 1100px) {

        .row {
            margin-top: 25%;
        }

  
    }
    
</style>
<div class="row" >
    <div class="col-md-12">

        <div class="card bg-danger" style="color:white">
            <div class="card-header">
                <h3 class="card-title">Logged out previous sessions</h3>
            </div>
            <div class="card-body">
                <a href="/account/login">   Click here to login again </a>
            </div>
            <!-- /.card-body -->
        </div>
    </div>
</div>
